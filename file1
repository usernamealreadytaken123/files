#include <stdio.h>
#include <stdlib.h>


size_t dedupl(int* arr, size_t length)
{
	for (int i = 0; i < length; i++)
	{
		if (arr[i + 1] == arr[i])
		{
			length = length - 1;
			for (int j = i; j < length; j++)
			{
				arr[j] = arr[j + 1];
			}
		}
	}
	return length;
}
int main()
{
	int n;

	scanf("%d", &n);
	int* arr = (int*)calloc(n, sizeof(int));
	int i = 0;
	for (int i = 0; i < n; i++)
	{
		scanf("%d", &arr[i]);
	}
	size_t length = n;
	length = dedupl(arr, length);

	for (int i = 0; i < length; i++)
	{
		printf("mass[%d]=%d\n", i, arr[i]);
	}

	free(arr);
	return 0;
}

